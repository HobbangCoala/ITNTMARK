<Window x:Class="ITNTMARK.WarningWindow2"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ITNTMARK"
        mc:Ignorable="d" Height="310" Width="660" WindowStartupLocation="CenterOwner" ResizeMode="NoResize">
    <Grid>
        <TextBlock x:Name ="tbkMessage1" HorizontalAlignment="Left" Height="65" Margin="30,40,0,0" VerticalAlignment="Top" Width="600" FontFamily="Global Sans Serif" FontWeight="Bold" FontSize="20" TextWrapping="Wrap"/>
        <!--<Label x:Name="lblConfrimMsg1" Content="" HorizontalAlignment="Left" Height="30" Margin="30,40,0,0" VerticalAlignment="Top" Width="600" FontFamily="Global Sans Serif" FontWeight="Bold" FontSize="20"/>
        <Label x:Name="lblConfrimMsg2" Content="" HorizontalAlignment="Left" Height="30" Margin="30,75,0,0" VerticalAlignment="Top" Width="600" FontFamily="Global Sans Serif" FontWeight="Bold" FontSize="20"/>-->
        <Label x:Name="lblConfrimMsg3" Content="" HorizontalAlignment="Left" Height="30" Margin="30,110,0,0" VerticalAlignment="Top" Width="600" FontFamily="Global Sans Serif" FontWeight="Bold" FontSize="20"/>
        <Label x:Name="lblConfrimMsg4" Content="" HorizontalAlignment="Left" Height="30" Margin="30,145,0,0" VerticalAlignment="Top" Width="600" FontFamily="Global Sans Serif" FontWeight="Bold" FontSize="20"/>
        <Label x:Name="lblConfrimMsg5" Content="" HorizontalAlignment="Left" Height="30" Margin="30,180,0,0" VerticalAlignment="Top" Width="600" FontFamily="Global Sans Serif" FontWeight="Bold" FontSize="20"/>

        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="0,230,0,0">
            <Button x:Name="btnRetry" Content="" Style="{StaticResource ITNTButton}"    HorizontalAlignment="Left" Margin="0,0,0,0" VerticalAlignment="Top" Width="120" Height="35" FontFamily="Global Sans Serif" FontSize="14" FontWeight="Bold" Click="btnRetry_Click"/>
            <Label x:Name="lblSpace1" Height="30" Width="60"/>
            <Button x:Name="btnContinue" Content="" Style="{StaticResource ITNTButton}" HorizontalAlignment="Left" Margin="0,0,0,0" VerticalAlignment="Top" Width="120" Height="35" FontFamily="Global Sans Serif" FontSize="14" FontWeight="Bold" Click="btnContinue_Click"/>
            <Label x:Name="lblSpace2" Height="30" Width="60"/>
            <Button x:Name="btnCancel" Content="" Style="{StaticResource ITNTButton}"   HorizontalAlignment="Left" Margin="0,0,0,0" VerticalAlignment="Top" Width="120" Height="35" FontFamily="Global Sans Serif" FontSize="14" FontWeight="Bold" Click="btnCancel_Click"/>
        </StackPanel>
    </Grid>
</Window>
