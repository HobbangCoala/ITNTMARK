<Window x:Class="ITNTMARK.SetControllerWindow2"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ITNTMARK"
        mc:Ignorable="d"
        Title="SetControllerWindow2" Height="900" Width="1600" Closing="Window_Closing" Closed="Window_Closed" Loaded="Window_Loaded" SizeChanged="Window_SizeChanged">
    <Grid>
        <TabControl x:Name="tabControl" HorizontalAlignment="Left" VerticalAlignment="Top" Width="1600" Height="875" >
            <TabItem Header="TEST" FontSize="18" FontWeight="Bold" Width="150" Height="40" VerticalAlignment="Top">
                <TabItem.Background>
                    <LinearGradientBrush EndPoint="0,1" StartPoint="0,0">
                        <GradientStop Color="#FFF0F0F0" Offset="0"/>
                        <GradientStop Color="#FF0F4AD3" Offset="1"/>
                    </LinearGradientBrush>
                </TabItem.Background>
                <Grid Margin="10,0,0,0">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="20*"/>
                        <ColumnDefinition Width="850*"/>
                        <ColumnDefinition Width="700*"/>
                        <ColumnDefinition Width="20*"/>
                        <!--<ColumnDefinition Width="915*"/>
                        <ColumnDefinition Width="661*"/>-->
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="50*"/>
                        <RowDefinition Height="140*"/>
                        <RowDefinition Height="100*"/>
                        <RowDefinition Height="80*"/>
                        <RowDefinition Height="290*"/>
                        <RowDefinition Height="190*"/>
                    </Grid.RowDefinitions>
                    <Grid Grid.Row="1" Grid.Column="1" Grid.ColumnSpan="2" >
                        <Grid x:Name="PlateColor" Height="110" Width="1273" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="0,10,0,0" Background="#FF566272"/>
                        <Label x:Name="lblDispMinXMaxY" Content="" HorizontalContentAlignment="Center" HorizontalAlignment="Left" Height="25" Margin="170,20,0,0" VerticalContentAlignment="Center" VerticalAlignment="Top" Width="100" BorderBrush="#FF566272" BorderThickness="1" Foreground="#FFF9180D" FontSize="10" Background="#FF566272" Padding="0"/>
                        <Label x:Name="lblDispMinXMinY" Content="" HorizontalContentAlignment="Center" HorizontalAlignment="Left" Height="25" Margin="170,85,0,0" VerticalContentAlignment="Center" VerticalAlignment="Top" Width="100" BorderBrush="#FF586270" BorderThickness="1" Foreground="#FFF9180D" FontSize="10" Background="#FF566272" Padding="0"/>
                        <Label x:Name="lblDispCenXMinY" Content="" HorizontalContentAlignment="Center" HorizontalAlignment="Left" Height="25" Margin="580,85,0,0" VerticalContentAlignment="Center" VerticalAlignment="Top" Width="100" BorderBrush="#FF566272" BorderThickness="1" Foreground="#FFF11F15" FontSize="10" Background="#FF566272" Padding="0"/>
                        <Label x:Name="lblDispMaxXMinY" Content="" HorizontalContentAlignment="Center" HorizontalAlignment="Left" Height="25" Margin="990,85,0,0" VerticalContentAlignment="Center" VerticalAlignment="Top" Width="100" BorderBrush="#FF566272" BorderThickness="1" Foreground="#FFF9180D" FontSize="10" Background="#FF566272" Padding="0"/>
                        <Label x:Name="lblDispMaxXMaxY" Content="" HorizontalContentAlignment="Center" HorizontalAlignment="Left" Height="25" Margin="990,20,0,0" VerticalContentAlignment="Center" VerticalAlignment="Top" Width="100" BorderBrush="#FF5D636B" BorderThickness="1" Foreground="#FFF1170D" FontSize="10" Background="#FF566272" Padding="0"/>
                        <Label x:Name="lblDispCenXMaxY" Content="" HorizontalContentAlignment="Center" HorizontalAlignment="Left" Height="25" Margin="580,20,0,0" VerticalContentAlignment="Center" VerticalAlignment="Top" Width="100" BorderBrush="#FF566272" BorderThickness="1" Foreground="#FFF9180D" FontSize="10" Background="#FF566272" Padding="0"/>
                        <Label x:Name="lblDispCenXCenY" Content="" HorizontalContentAlignment="Center" HorizontalAlignment="Left" Height="25" Margin="580,53,0,0" VerticalContentAlignment="Center" VerticalAlignment="Top" Width="100" BorderBrush="#FF576272" BorderThickness="1" FontSize="10" FontWeight="Bold" Padding="0"/>

                        <Grid x:Name="GridPallete" HorizontalAlignment="Left" Height="110" Margin="1280,10,0,0" VerticalAlignment="Top" Width="35" Background="{DynamicResource {x:Static SystemColors.ControlDarkDarkBrushKey}}" />
                        <Label x:Name="TxtMaxOffset" Content="+0.50" HorizontalAlignment="Left" Margin="1320,0,0,0" VerticalAlignment="Top" HorizontalContentAlignment="Left" Foreground="#FFFF4B00" Height="30" Width="60" Padding="0" Grid.Row="1" Grid.Column="2"/>
                        <Label x:Name="TxtZeroOffset" Content="  0" HorizontalAlignment="Left" Margin="1320,52,0,0" VerticalAlignment="Top" HorizontalContentAlignment="Left" FontSize="18" FontWeight="Bold" Foreground="#FF1C8631" Height="30" Width="60" Padding="0"/>
                        <Label x:Name="TxtMinOffset" Content="-0.50" HorizontalAlignment="Left" Margin="1320,100,0,0" VerticalAlignment="Top" HorizontalContentAlignment="Left" Foreground="#FF00B4FF" Height="30" Width="60" Padding="0"/>

                        <Label x:Name="lblDisplacementVal1" Content="" HorizontalContentAlignment="Center" HorizontalAlignment="Left" Height="40" Margin="1400,20,0,0" VerticalContentAlignment="Center" VerticalAlignment="Top" Width="106" BorderBrush="#FFB01717" BorderThickness="1" FontSize="16"/>
                        <Label x:Name="lblDisplacementVal2" Content="" HorizontalContentAlignment="Center" HorizontalAlignment="Left" Height="40" Margin="1400,70,0,0" VerticalContentAlignment="Center" VerticalAlignment="Top" Width="106" BorderBrush="Black" BorderThickness="1" FontSize="18" FontWeight="Bold" Foreground="#FF151010"/>
                    </Grid>
                    <Grid Grid.Row="2" Grid.Column="1" Grid.ColumnSpan="2"  >
                        <StackPanel Orientation="Horizontal" Margin="0,5,0,0" Height="85" Width="1280" HorizontalAlignment="Left" VerticalAlignment="Top">
                            <Canvas x:Name="cvsshowChar00" Background="DarkGray" Height="85" VerticalAlignment="Top" Width="67"  />
                            <Canvas x:Name="cvsshowChar01" Background="DarkGray" Height="85" VerticalAlignment="Top" Width="67"  />
                            <Canvas x:Name="cvsshowChar02" Background="DarkGray" Height="85" VerticalAlignment="Top" Width="67"  />
                            <Canvas x:Name="cvsshowChar03" Background="DarkGray" Height="85" VerticalAlignment="Top" Width="67"  />
                            <Canvas x:Name="cvsshowChar04" Background="DarkGray" Height="85" VerticalAlignment="Top" Width="67"  />
                            <Canvas x:Name="cvsshowChar05" Background="DarkGray" Height="85" VerticalAlignment="Top" Width="67"  />
                            <Canvas x:Name="cvsshowChar06" Background="DarkGray" Height="85" VerticalAlignment="Top" Width="67"  />
                            <Canvas x:Name="cvsshowChar07" Background="DarkGray" Height="85" VerticalAlignment="Top" Width="67"  />
                            <Canvas x:Name="cvsshowChar08" Background="DarkGray" Height="85" VerticalAlignment="Top" Width="67"  />
                            <Canvas x:Name="cvsshowChar09" Background="DarkGray" Height="85" VerticalAlignment="Top" Width="67"  />
                            <Canvas x:Name="cvsshowChar10" Background="DarkGray" Height="85" VerticalAlignment="Top" Width="67"  />
                            <Canvas x:Name="cvsshowChar11" Background="DarkGray" Height="85" VerticalAlignment="Top" Width="67"  />
                            <Canvas x:Name="cvsshowChar12" Background="DarkGray" Height="85" VerticalAlignment="Top" Width="67"  />
                            <Canvas x:Name="cvsshowChar13" Background="DarkGray" Height="85" VerticalAlignment="Top" Width="67"  />
                            <Canvas x:Name="cvsshowChar14" Background="DarkGray" Height="85" VerticalAlignment="Top" Width="67"  />
                            <Canvas x:Name="cvsshowChar15" Background="DarkGray" Height="85" VerticalAlignment="Top" Width="67"  />
                            <Canvas x:Name="cvsshowChar16" Background="DarkGray" Height="85" VerticalAlignment="Top" Width="67"  />
                            <Canvas x:Name="cvsshowChar17" Background="DarkGray" Height="85" VerticalAlignment="Top" Width="67"  />
                            <Canvas x:Name="cvsshowChar18" Background="DarkGray" Height="85" VerticalAlignment="Top" Width="67"  />
                        </StackPanel>
                    </Grid>
                    <Grid Grid.Row="3" Grid.Column="1" Margin="0,0,0,0">
                        <Label Content="WaveFrom Profile" HorizontalAlignment="Left" HorizontalContentAlignment="Center" Height="29" Margin="0,-4,0,0" VerticalAlignment="Top" Width="139" FontSize="14" Padding="0,5,0,0" Visibility="Collapsed"/>
                        <TextBox x:Name="txtCurrProfile" HorizontalAlignment="Left" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Height="30" Margin="27,27,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="64"  FontSize="18" Visibility="Collapsed"/>
                        <Label Content="VIN : " HorizontalAlignment="Left" HorizontalContentAlignment="Right" Height="50" Margin="100,15,0,0" VerticalAlignment="Top" Width="150" FontSize="30"/>
                        <TextBox x:Name="txtVIN" HorizontalAlignment="Left" HorizontalContentAlignment="Center" Height="50" Margin="250,15,0,0" TextWrapping="Wrap" Text="ABCDEFGHIJKLMNOPQRS" VerticalAlignment="Top" Width="500" Background="#FF816565" FontSize="30" TextChanged="txtVIN_TextChanged"/>
                        <Label x:Name="lblVINLength" Content="" HorizontalAlignment="Left" HorizontalContentAlignment="Center" Height="50" Margin="770,15,0,0" VerticalAlignment="Top" Width="50" FontSize="30" FontWeight="Bold" Background="#FF958787"/>
                    </Grid>
                    <Grid Grid.Row="4" Grid.Column="1" Margin="0">
                        <Button x:Name="btnServoOnOff" Content="SERVO ON"     HorizontalAlignment="Left" Height="30" Margin="20,5,0,0" VerticalAlignment="Top" Width="120" Click="btnServoOnOff_Click" FontSize="14"/>
                        <Button x:Name="btnLaserInfo"  Content="LASER INFO"   HorizontalAlignment="Left" Height="30" Margin="20,45,0,0" VerticalAlignment="Top" Width="120" Click="btnLaserInfo_Click" FontSize="14"/>
                        <Button x:Name="btnLaserReset" Content="RESET LASER" HorizontalAlignment="Left" Height="30" Margin="20,85,0,0" VerticalAlignment="Top" Width="120" Click="btnLaserReset_Click" FontSize="14"/>
                        <Button x:Name="btnFirmware"   Content="FIRMWARE"     HorizontalAlignment="Left" Height="30" Margin="20,125,0,0" VerticalAlignment="Top" Width="120" FontSize="14" Click="btnFirmware_Click" />
                        <!--<Button x:Name="btnFixDotMissing" Content="DOT MISSING" HorizontalAlignment="Left" Height="30" Margin="50,125,0,0" VerticalAlignment="Top" Width="120" FontSize="14" Click="btnFixDotMissing_Click"/>-->

                        <Button x:Name="btnGoPark"      Content="GO PARKING" HorizontalAlignment="Left" Height="30" Margin="180,5,0,0"  VerticalAlignment="Top" Width="120" FontSize="14" Click="btnGoPark_Click" />
                        <Button x:Name="btnCenterPoint" Content="GO CENTER" HorizontalAlignment="Left" Height="30" Margin="180,45,0,0" VerticalAlignment="Top" Width="120" FontSize="14" Click="btnCenterPoint_Click" Visibility="Collapsed"/>
                        <Button x:Name="btnMarkPoint"   Content="MARK POSITION" HorizontalAlignment="Left" Height="30" Margin="180,45,0,0" VerticalAlignment="Top" Width="120" FontSize="14" Click="btnMarkPoint_Click" />
                        <Button x:Name="btnGoHome"      Content="GO HOME"      HorizontalAlignment="Left" Height="30" Margin="180,85,0,0" VerticalAlignment="Top" Width="120" FontSize="14" Click="btnGoHome_Click" />

                        <Button x:Name="btnReadFont"  Content="READ FONT"  HorizontalAlignment="Left" Height="30" Margin="340,5,0,0" VerticalAlignment="Top" Width="120" Click="btnReadFont_Click" FontSize="14"/>
                        <Button x:Name="btnAreaCheck" Content="PLATE CHECK" HorizontalAlignment="Left" Height="30" Margin="340,45,0,0" VerticalAlignment="Top" Width="120" Click="btnAreaCheck_Click" FontSize="14"/>
                        <CheckBox x:Name="ckbRepeatCheck" Content="CONTINOUS" HorizontalAlignment="Left" Height="15" Margin="405,75,0,0" VerticalAlignment="Top" Width="77" FontSize="10" HorizontalContentAlignment="Left" VerticalContentAlignment="Center" Padding="0,0,0,0"/>
                        <CheckBox x:Name="ckbErrHeight" Content="HEIGHT" HorizontalAlignment="Left" Height="15" Margin="340,75,0,0" VerticalAlignment="Top" Width="65" FontSize="10" HorizontalContentAlignment="Left" VerticalContentAlignment="Center" Padding="0,0,0,0"/>
                        <CheckBox x:Name="ckbErrCline" Content="SLOPE" HorizontalAlignment="Left" Height="15" Margin="340,90,0,0" VerticalAlignment="Top" Width="80" FontSize="10" HorizontalContentAlignment="Left" VerticalContentAlignment="Center" Padding="0,0,0,0"/>
                        <TextBox x:Name="txtboxCheckRepeatCount" HorizontalAlignment="Left" Height="30" Margin="460,45,0,0" TextWrapping="Wrap" Text="1" VerticalAlignment="Top" Width="25" FontSize="14" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Visibility="Collapsed"/>
                        <!--<Button x:Name="btnCalculateData" Content="MAKE DATA" HorizontalAlignment="Left" Height="30" Margin="430,45,0,0" VerticalAlignment="Top" Width="120" Click="btnCalculateData_Click" FontSize="14"/>-->
                        <!--<Button x:Name="btnMarkingConer" Content="MARKING CONER" HorizontalAlignment="Left" Height="30" Margin="430,125,0,0" VerticalAlignment="Top" Width="120" Click="btnMarkingConer_Click" FontSize="13"/>-->

                        <Button x:Name="btnDryRun"  Content="DRY RUN"  HorizontalAlignment="Left" Height="30" Margin="500,5,0,0" VerticalAlignment="Top" Width="120" Click="btnDryRun_Click" FontSize="14" Visibility="Collapsed"/>
                        <Button x:Name="btnMarkRun" Content="RUN MARKING" HorizontalAlignment="Left" Height="30" Margin="500,45,0,0" VerticalAlignment="Top" Width="120" Click="btnMarkRun_Click" FontSize="14" />
                        <CheckBox x:Name="ckbSkipCheckPlane" Content="SKIP PLATE" HorizontalAlignment="Left" Height="15" Margin="500,75,0,0" VerticalAlignment="Top" Width="96" FontSize="10"/>
                        <!--<CheckBox x:Name="ckbAging" Content="Aging Test" HorizontalAlignment="Left" Height="20" Margin="710,75,0,0" VerticalAlignment="Top" Width="85" FontSize="12"/>-->
                        <TextBox x:Name="tbxRunRepeatCount" HorizontalAlignment="Left" Height="30" Margin="620,45,0,0" TextWrapping="Wrap" Text="1" VerticalAlignment="Top" Width="25" FontSize="14" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"/>

                        <Button x:Name="btnGoPoint" Content="GO TO BELOW" HorizontalAlignment="Left" Height="30" Margin="180,125,0,0" VerticalAlignment="Top" Width="120" Click="btnGoPoint_Click" FontSize="14"/>
                        <TextBox x:Name="txtGoPoint_X" HorizontalAlignment="Left" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Height="30" Margin="150,155,0,0" VerticalAlignment="Top" Width="60"  FontSize="12"/>
                        <TextBox x:Name="txtGoPoint_Y" HorizontalAlignment="Left" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Height="30" Margin="210,155,0,0" VerticalAlignment="Top" Width="60"  FontSize="12"/>
                        <TextBox x:Name="txtGoPoint_Z" HorizontalAlignment="Left" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Height="30" Margin="270,155,0,0" VerticalAlignment="Top" Width="60"  FontSize="12"/>
                        <Button x:Name="btnSetStartPoint" Content="GO CENTER" HorizontalAlignment="Left" Height="30" Margin="180,185,0,0" VerticalAlignment="Top" Width="120" Click="btnSetStartPoint_Click" FontSize="14"/>

                        <Button x:Name="btnPINTest" Content="LASER ONE SPOT" HorizontalAlignment="Left" Height="30" Margin="500,5,0,0" VerticalAlignment="Top" Width="120" Click="btnPINTest_Click" FontSize="12"/>

                        <Label x:Name="lblCommStatus"   Content="1"             HorizontalAlignment="Left" Height="30" Margin="851,237,0,0" VerticalAlignment="Top" Width="30" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Background="#FF2AEC17"/>
                        <Button x:Name="btnAorOn"       Content="AIR ON"        HorizontalAlignment="Left" Height="30" Margin="340,125,0,0" VerticalAlignment="Top" Width="120" FontSize="14" Click="btnAirOn_Click"/>
                        <Button x:Name="btnAirOff"      Content="AIR OFF"       HorizontalAlignment="Left" Height="30" Margin="340,165,0,0" VerticalAlignment="Top" Width="120" FontSize="14" Click="btnAirOff_Click" />
                        <Button x:Name="btnAimingOn"    Content="AIMING ON"     HorizontalAlignment="Left" Height="30" Margin="500,125,0,0" VerticalAlignment="Top" Width="120" FontSize="14" Click="btnAimingOn_Click" />
                        <Button x:Name="btnAimingOff"   Content="AIMING OFF"    HorizontalAlignment="Left" Height="30" Margin="500,165,0,0" VerticalAlignment="Top" Width="120" FontSize="14" Click="btnAimingOff_Click" />
                        <Button x:Name="btnMarkCorner"  Content="MARK CORNER"   HorizontalAlignment="Left" Height="30" Margin="340,205,0,0" VerticalAlignment="Top" Width="120" FontSize="14" Click="btnMarkCorner_Click" />
                    </Grid>
                    <Grid Grid.Row="4" Grid.Column="2" >
                        <StackPanel Orientation="Horizontal">
                            <Label x:Name="AvgPowerLabel" Content="AVERAGE POWER" HorizontalAlignment="Left" Margin="5,0,0,0" VerticalAlignment="Top" VerticalContentAlignment="Center" Height="30" Width="120" FontSize="14" Padding="0"/>
                            <TextBox x:Name="AvgPowerTxt" HorizontalAlignment="Left" Height="30" TextAlignment="Center" Margin="0,0,0,0" TextWrapping="Wrap" Text="OFF" VerticalAlignment="Top" Width="100" FontSize="20" Background="{DynamicResource {x:Static SystemColors.ActiveBorderBrushKey}}" FontFamily="Yu Gothic UI Semibold"/>
                            <Label Content="(Watts)" HorizontalAlignment="Left" Margin="0,0,0,0" VerticalAlignment="Top" VerticalContentAlignment="Center" Height="30" Width="60" FontSize="14" Padding="0"/>
                            <Label HorizontalAlignment="Left" Margin="0,0,0,0" VerticalAlignment="Top" VerticalContentAlignment="Center" Height="30" Width="20" FontSize="12" Padding="0"/>
                            <Label x:Name="TemperatureLabel" Content="TEMPERATURE" HorizontalAlignment="Left" Margin="0,0,0,0" VerticalAlignment="Top" Height="30" Width="120" FontSize="16" VerticalContentAlignment="Center" Padding="0"/>
                            <TextBox x:Name="TemperatureTxt" HorizontalAlignment="Left" Height="30" TextAlignment="Center" Margin="0,0,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" Width="70" Background="{DynamicResource {x:Static SystemColors.ActiveBorderBrushKey}}" FontSize="16" FontFamily="Yu Gothic UI Semibold"/>
                            <Label Content="(°C)" HorizontalAlignment="Left" Margin="0,0,0,0" VerticalAlignment="Top" Height="30" Width="80" FontSize="14" VerticalContentAlignment="Center" Padding="0"/>
                        </StackPanel>
                        <StackPanel Orientation="Horizontal" Margin="0,40,0,0">
                            <Label x:Name="PeakPowewrLabel" Content="MAX POWER" HorizontalAlignment="Left" HorizontalContentAlignment="Right" Margin="5,0,0,0" VerticalAlignment="Top" VerticalContentAlignment="Center" Height="30" Width="120" FontSize="14" Padding="0"/>
                            <TextBox x:Name="PeakPowerTxt" HorizontalAlignment="Left" Height="30" TextAlignment="Center" Margin="0,0,0,0" TextWrapping="Wrap" Text="OFF" VerticalAlignment="Top" Width="100" Background="{DynamicResource {x:Static SystemColors.ActiveBorderBrushKey}}" FontSize="20" FontFamily="Yu Gothic UI Semibold"/>
                            <Label Content="(Watts)" HorizontalAlignment="Left" Margin="0,0,0,0" VerticalAlignment="Top" VerticalContentAlignment="Center" Height="30" Width="60" FontSize="14" Padding="0"/>
                            <Label HorizontalAlignment="Left" Margin="0,0,0,0" VerticalAlignment="Top" VerticalContentAlignment="Center" Height="30" Width="20" FontSize="12" Padding="0"/>
                            <Label x:Name="LaserErrorLabel" Content="LASER STATUS" HorizontalAlignment="Left" Margin="0,0,0,0" VerticalAlignment="Top" VerticalContentAlignment="Center" Width="120" FontSize="16" Height="30" Padding="0"/>
                            <TextBox x:Name="LaserErrorTxt" HorizontalAlignment="Left" HorizontalContentAlignment="Center" TextAlignment="Center" VerticalContentAlignment="Center" Margin="0,0,0,0" TextWrapping="Wrap" Text="Error Code" Height="30" Width="145" FontSize="16" BorderBrush="#FFF5143D" BorderThickness="0,0,2,2" Foreground="Black" Background="#FFB4B4B4" VerticalAlignment="Top" MouseDoubleClick="LaserErrorTxt_MouseDoubleClick"/>
                        </StackPanel>
                        <StackPanel Orientation="Horizontal" Margin="0,80,0,0">
                            <Label Content="MARKING POSITION" HorizontalAlignment="Right" VerticalAlignment="Top" Margin="5,0,0,0" Width="150" Height="30" FontSize="14"/>
                            <RadioButton x:Name="rbtMarkingPos1" GroupName="MarkingPos" Content="SEAT CROSS" HorizontalAlignment="Left" VerticalContentAlignment="Center" Margin="0,0,0,0" VerticalAlignment="Top" Padding="0" Height="30" Width="150" IsChecked="True" FontSize="14" />
                            <RadioButton x:Name="rbtMarkingPos2" GroupName="MarkingPos" Content="APRON" HorizontalAlignment="Left" VerticalContentAlignment="Center" Margin="0,0,0,0" VerticalAlignment="Top" Padding="0" Height="30" Width="150" FontSize="14"/>
                        </StackPanel>

                        <GroupBox x:Name="groupBox" Header="MOTOR CONTROLL" HorizontalAlignment="Left" HorizontalContentAlignment="Center"  Height="160" Margin="10,120,0,0" VerticalAlignment="Top" Width="607" FontSize="16" BorderBrush="Black" VerticalContentAlignment="Center"/>
                        <StackPanel Orientation="Horizontal" Margin="12,152,0,0" HorizontalAlignment="Left" VerticalAlignment="Top">
                            <StackPanel Orientation="Vertical" Height="120">
                                <Label Content="DISTANCE" HorizontalAlignment="Left" Height="25" Margin="20,0,0,0" VerticalAlignment="Top" Width="120" FontSize="14" HorizontalContentAlignment="Left" VerticalContentAlignment="Center" FontWeight="Bold" Padding="0"/>
                                <RadioButton x:Name="rbt01mm" GroupName="XYAxis" Content=" 0.1mm" HorizontalAlignment="Left" VerticalContentAlignment="Center" Margin="20,0,0,0" VerticalAlignment="Top" Padding="0" FontSize="14" Height="20" Width="70" />
                                <RadioButton x:Name="rbt10mm" GroupName="XYAxis" Content="   1mm" HorizontalAlignment="Left" VerticalContentAlignment="Center" Margin="20,0,0,0" VerticalAlignment="Top" Padding="0" FontSize="14" Height="20" Width="70" IsChecked="True"/>
                                <RadioButton x:Name="rbt20mm" GroupName="XYAxis" Content="   2mm" HorizontalAlignment="Left" VerticalContentAlignment="Center" Margin="20,0,0,0" VerticalAlignment="Top" Padding="0" FontSize="14" Height="20" Width="70" />
                                <RadioButton x:Name="rbt30mm" GroupName="XYAxis" Content="   3mm" HorizontalAlignment="Left" VerticalContentAlignment="Center" Margin="20,0,0,0" VerticalAlignment="Top" Padding="0" FontSize="14" Height="20" Width="70" />
                            </StackPanel>
                            <StackPanel Orientation="Vertical" Height="120">
                                <Label Content="CURRENT POSITION" HorizontalAlignment="Left" Height="25" Width="138" FontSize="14" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontWeight="Bold" Padding="0"/>
                                <StackPanel Orientation="Horizontal">
                                    <Label Content="X" HorizontalAlignment="Left" Height="25" Margin="5,0,0,0" VerticalAlignment="Top" Width="30" FontSize="14" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontWeight="Bold" Padding="0"/>
                                    <TextBox x:Name="txtCurrentPosX" HorizontalAlignment="Left" Height="25" Margin="5,0,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="55" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="16" Background="#FFD1BCBC"/>
                                    <Label Content="(mm)" HorizontalAlignment="Left" Height="25" Margin="5,0,0,0" VerticalAlignment="Top" Width="40" FontSize="14" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontWeight="Bold" Padding="0" RenderTransformOrigin="0.593,-0.487"/>
                                </StackPanel>
                                <StackPanel Orientation="Horizontal">
                                    <Label Content="Y" HorizontalAlignment="Left" Height="25" Margin="5,0,0,0" VerticalAlignment="Top" Width="30" FontSize="14" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontWeight="Bold" Padding="0"/>
                                    <TextBox x:Name="txtCurrentPosY" HorizontalAlignment="Left" Height="25" Margin="5,0,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="55" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="16" Background="#FFD1BCBC" />
                                    <Label Content="(mm)" HorizontalAlignment="Left" Height="25" Margin="5,0,0,0" VerticalAlignment="Top" Width="40" FontSize="14" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontWeight="Bold" Padding="0"/>
                                </StackPanel>
                                <StackPanel Orientation="Horizontal">
                                    <Label Content="Z" HorizontalAlignment="Left" Height="25" Margin="5,0,0,0" VerticalAlignment="Top" Width="30" FontSize="14" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontWeight="Bold" Padding="0"/>
                                    <TextBox x:Name="txtCurrentPosZ" HorizontalAlignment="Left" Height="25" Margin="5,0,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="55" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="16" Background="#FFD1BCBC" />
                                    <Label Content="(mm)" HorizontalAlignment="Left" Height="25" Margin="5,0,0,0" VerticalAlignment="Top" Width="40" FontSize="14" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontWeight="Bold" Padding="0"/>
                                </StackPanel>
                            </StackPanel>
                            <Label Width="30"/>
                            <StackPanel Orientation="Horizontal">
                                <Button x:Name="btnMoveYMinus" Content="Y-" HorizontalAlignment="Left" Height="40"  VerticalAlignment="Center" Width="30" FontSize="16" FontWeight="Bold" Click="btnMoveYMinus_Click"/>
                                <StackPanel Orientation="Vertical">
                                    <Button x:Name="btnMoveXMinus" Content="X-" HorizontalAlignment="Center" Height="40"  VerticalAlignment="Top" Width="30" FontSize="16" FontWeight="Bold" Click="btnMoveXMinus_Click"/>
                                    <Image x:Name="imgBody" HorizontalAlignment="Left" Height="40"  VerticalAlignment="Top" Width="80" Source="Image/BodyReverse.jpg" />
                                    <Button x:Name="btnMoveXPlus" Content="X+" HorizontalAlignment="Center" Height="40"  VerticalAlignment="Top" Width="30" FontSize="16" FontWeight="Bold" Click="btnMoveXPlus_Click"/>
                                </StackPanel>
                                <Button x:Name="btnMoveYPlus" Content="Y+" HorizontalAlignment="Left" Height="40"  VerticalAlignment="Center" Width="30" FontSize="16" FontWeight="Bold" Click="btnMoveYPlus_Click"/>
                            </StackPanel>
                            <Label Width="30"/>
                            <StackPanel Orientation="Vertical">
                                <Label Content="" HorizontalAlignment="Center" Height="16" Width="30"/>
                                <Image x:Name="imgHead" VerticalAlignment="Top" Height="90"  Width="48" Source="/Image/MarkingHead.png" />
