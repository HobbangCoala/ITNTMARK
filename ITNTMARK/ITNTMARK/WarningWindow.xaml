<Window x:Class="ITNTMARK.WarningWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ITNTMARK"
        Title="Warning Window"
        mc:Ignorable="d" Height="310" Width="660" WindowStartupLocation="CenterOwner" ResizeMode="NoResize">
    <Grid>
        <Label x:Name="lblConfrimMsg1" Content="" HorizontalAlignment="Left" Height="30" Margin="30,40,0,0" VerticalAlignment="Top" Width="600" FontFamily="Global Sans Serif" FontWeight="Bold" FontSize="20"/>
        <Label x:Name="lblConfrimMsg2" Content="" HorizontalAlignment="Left" Height="30" Margin="30,75,0,0" VerticalAlignment="Top" Width="600" FontFamily="Global Sans Serif" FontWeight="Bold" FontSize="20"/>
        <Label x:Name="lblConfrimMsg3" Content="" HorizontalAlignment="Left" Height="30" Margin="30,110,0,0" VerticalAlignment="Top" Width="600" FontFamily="Global Sans Serif" FontWeight="Bold" FontSize="20"/>
        <Label x:Name="lblConfrimMsg4" Content="" HorizontalAlignment="Left" Height="30" Margin="30,145,0,0" VerticalAlignment="Top" Width="600" FontFamily="Global Sans Serif" FontWeight="Bold" FontSize="20"/>
        <Label x:Name="lblConfrimMsg5" Content="" HorizontalAlignment="Left" Height="30" Margin="30,180,0,0" VerticalAlignment="Top" Width="600" FontFamily="Global Sans Serif" FontWeight="Bold" FontSize="20"/>

        <Button x:Name="btnOK" Content="" Style="{StaticResource ITNTButton}" HorizontalAlignment="Left" Margin="160,230,0,0" VerticalAlignment="Top" Width="120" Height="35" IsDefault="True" FontFamily="Global Sans Serif" FontSize="14" FontWeight="Bold" Click="btnOK_Click"/>
        <Button x:Name="btnCancel" Content="" Style="{StaticResource ITNTButton}" HorizontalAlignment="Left" Margin="380,230,0,0" VerticalAlignment="Top" Width="120" Height="35" FontSize="14" FontFamily="Global Sans Serif" FontWeight="Bold" IsDefault="True" IsCancel="True" Click="btnCancel_Click"/>
        <!--Button x:Name="btnOK2" Content="" Style="{StaticResource ITNTButton}" HorizontalAlignment="Left" Margin="224,180,0,0" VerticalAlignment="Top" Width="105" Height="37" IsDefault="True" FontFamily="Global Sans Serif" FontSize="14" FontWeight="Bold" Visibility="Hidden" IsCancel="True"/>
        <<Button x:Name="btnOK" Content="" HorizontalAlignment="Left" VerticalAlignment="Top" Height="35" Width="100" FontSize="14" FontWeight="Bold" IsDefault="True" Visibility="Visible" Click="btnOK_Click" Margin="155,181,0,0" />
        <Button x:Name="btnCancel" Content="" HorizontalAlignment="Left" VerticalAlignment="Top" Height="35" Width="100" FontSize="14" FontWeight="Bold" IsDefault="True" IsCancel="True" Visibility="Visible" Click="btnCancel_Click" Margin="305,181,0,0"/>-->
    </Grid>
</Window>
